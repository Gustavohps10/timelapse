{
  "name": "@timelapse/addon-for-tests",
  "version": "1.0.1",
  "description": "Addon for tests.",
  "author": "Gustavo gustavoh.santos735@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --dts",
