    "pkg": "timelapse pkg ./dist --config ./timelapse.json"
  },
  "dependencies": {
    "@timelapse/connector-sdk": "workspace:*",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@types/node": "^24.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
