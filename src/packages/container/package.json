{
  "name": "@timelapse/container",
  "version": "1.0.0",
  "private": true,
  "description": "Construtor de container de Injeção de Dependência para o ecossistema Timelapse.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup --format esm,cjs --dts",
    "clean": "rimraf dist .turbo"
  },
  "dependencies": {
    "@timelapse/application": "workspace:*",
    "@timelapse/infra": "workspace:*",
    "awilix": "^12.0.5"
  }
}
