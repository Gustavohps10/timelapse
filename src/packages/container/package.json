{
  "name": "@trackalize/container",
  "version": "1.0.0",
  "private": true,
  "description": "Construtor de container de Injeção de Dependência para o ecossistema Trackalize.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup --format esm,cjs --dts",
    "clean": "rimraf dist .turbo"
  },
  "dependencies": {
    "@trackalize/application": "workspace:*",
    "@trackalize/infra": "workspace:*",
    "awilix": "^12.0.5"
  }
}
