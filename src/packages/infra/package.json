{
  "name": "@trackalize/infra",
  "version": "1.0.0",
  "private": true,
  "description": "Pacote de infraestrutura consolidado para o Trackalize, contendo implementações concretas de recursos externos",
  "type": "module",
  "scripts": {
    "build": "tsup --format esm,cjs --dts"
  },
  "exports": {
    "./auth": {
      "import": "./dist/auth/index.js",
      "types": "./dist/auth/index.d.ts"
    },
    "./contracts": {
      "import": "./dist/contracts/index.js",
      "types": "./dist/contracts/index.d.ts"
    },
    "./http": {
      "import": "./dist/http/index.js",
      "types": "./dist/http/index.d.ts"
    },
    "./storage": {
      "import": "./dist/storage/index.js",
      "types": "./dist/storage/index.d.ts"
    },
    "./workflow": {
      "import": "./dist/workflow/index.js",
      "types": "./dist/workflow/index.d.ts"
    },
    "./data": {
      "import": "./dist/data/index.js",
      "types": "./dist/data/index.d.ts"
    }
  },
  "dependencies": {
    "@trackalize/application": "workspace:*",
    "@trackalize/cross-cutting": "workspace:*",
    "@trackalize/domain": "workspace:*",
    "axios": "^1.7.9",
    "jsonwebtoken": "^9.0.2",
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "typescript": "^5.8.3"
  }
}
