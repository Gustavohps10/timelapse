{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/components/**/*",
    "src/renderer/lib/**/*",
    "src/renderer/contexts/**/*",
    "src/renderer/hooks/**/*",
    "src/renderer/utils/**/*",
    "src/main/api/**/*",
    "src/main/lib/**/*",
    "src/main/types/**/*",
    "src/main/utils/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts", "src/main/lib/axios.ts",
    //"src/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@/*": ["./src/*"]
    }
  }
}
